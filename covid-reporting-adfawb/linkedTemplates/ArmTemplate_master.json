{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-reporting-adfawb"
		},
		"ls_ablob_covidreportingawb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidreportingawb'"
		},
		"ls_adls_covidreportingkk_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportingkk'"
		},
		"ls_adls_covidreportingkk_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingkk.dfs.core.windows.net/"
		},
		"ls_http_opendata_europa_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com/"
		},
		"tr_ingest_hospital_admission_data_properties_pl_ingest_ecdc_data_parameters_SourcerelativeURL": {
			"type": "string",
			"defaultValue": "cloudboxacademy/covid19/raw/main/ecdc_data/hospital_admissions.csv"
		},
		"tr_ingest_hospital_admission_data_properties_pl_ingest_ecdc_data_parameters_sinkFileName": {
			"type": "string",
			"defaultValue": "hospital_admissions.csv"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-reporting-adfawb_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidreportingawb_connectionString": {
						"value": "[parameters('ls_ablob_covidreportingawb_connectionString')]"
					},
					"ls_adls_covidreportingkk_accountKey": {
						"value": "[parameters('ls_adls_covidreportingkk_accountKey')]"
					},
					"ls_adls_covidreportingkk_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportingkk_properties_typeProperties_url')]"
					},
					"ls_http_opendata_europa_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_europa_eu_properties_typeProperties_url')]"
					},
					"tr_ingest_hospital_admission_data_properties_pl_ingest_ecdc_data_parameters_SourcerelativeURL": {
						"value": "[parameters('tr_ingest_hospital_admission_data_properties_pl_ingest_ecdc_data_parameters_SourcerelativeURL')]"
					},
					"tr_ingest_hospital_admission_data_properties_pl_ingest_ecdc_data_parameters_sinkFileName": {
						"value": "[parameters('tr_ingest_hospital_admission_data_properties_pl_ingest_ecdc_data_parameters_sinkFileName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}